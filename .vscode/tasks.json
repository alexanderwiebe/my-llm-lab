{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Start Jupyter Lab",
			"command": "/usr/local/bin/jupyter",
			"args": [
				"lab",
				"--ip=0.0.0.0",
				"--port=8888",
				"--no-browser",
				"--allow-root",
				"--PasswordIdentityProvider.hashed_password=''"
			],
			"group": "none",
			"isBackground": true,
			"problemMatcher": []
		},
		{
			"type": "shell",
			"label": "Which Jupyter",
			"command": "which jupyter",
			"group": "none",
			"isBackground": false,
			"problemMatcher": []
		},
		{
			"type": "shell",
			"label": "Echo PATH",
			"command": "echo $PATH",
			"group": "none",
			"isBackground": false,
			"problemMatcher": []
		}
	]
}